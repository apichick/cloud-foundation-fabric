<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
  <Flows>
    <Flow name="Ping">
      <Response>
        <Step>
          <Name>AM-Response</Name>
        </Step>
      </Response>
      <Condition>request.verb = "GET" AND proxy.pathsuffix = "/ping"</Condition>
    </Flow>
  </Flows>
  <HTTPProxyConnection>
    <BasePath>/dummy</BasePath>
  </HTTPProxyConnection>
  <RouteRule name="default"/>
</ProxyEndpoint>
